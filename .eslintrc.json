{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "indent": [2, "tab", {
      "MemberExpression": 0
    }],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-multi-spaces": ["warn"],
    "space-infix-ops": ["warn"],
    "no-console": 0,
    "no-unused-vars": ["warn", { "args": "none" }]
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "All": true,
    "HttpError": true,
    "Text": true,
    "Log": true
  }
}
